{
    "name": "cars-tournaments-admin",
    "version": "0.0.4",
    "author": "JoseXS",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --configuration production --base-href /admin/",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
        "prettier:write": "prettier --write \"./{e2e,src}/**/*.{scss,ts}\"",
        "lint": "eslint \"**/*.{ts,tsx}\" --fix",
        "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/cars-tournaments-admin/stats.json",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/cdk": "^13.3.4",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/flex-layout": "^13.0.0-beta.38",
        "@angular/forms": "~13.2.0",
        "@angular/material": "^13.3.4",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "@ng-matero/extensions": "^13.1.0",
        "@ng-matero/extensions-moment-adapter": "^13.0.0",
        "@ngx-formly/core": "^5.12.0",
        "@ngx-formly/material": "^5.12.0",
        "animate.css": "^4.1.1",
        "bootstrap": "^5.1.3",
        "chart.js": "^3.7.1",
        "husky": "^7.0.4",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.34",
        "ng-circle-progress": "^1.6.0",
        "ng2-charts": "^3.0.11",
        "ngx-permissions": "^13.0.1",
        "ngx-progressbar": "^8.0.0",
        "ngx-toastr": "^14.3.0",
        "photoviewer": "^3.6.2",
        "rxjs": "~7.5.0",
        "swiper": "^8.1.5",
        "tslib": "^2.3.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.6",
        "@angular/cli": "~13.3.6",
        "@angular/compiler-cli": "~13.2.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.8.0",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "lint": "^0.7.0",
        "prettier": "^2.6.0",
        "pretty-quick": "^3.1.3",
        "typescript": "~4.5.2"
    },
    "eslintIgnore": [
        "*.spec.ts"
    ]
